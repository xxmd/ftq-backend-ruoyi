<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.clash.mapper.ProxyGroupNodeMapper">
    <insert id="batchInsert">
        insert into proxy_group_node(group_id, node_id) values
        <foreach item="item" index="index" collection="list" separator=",">
            (#{item.groupId},#{item.nodeId})
        </foreach>
    </insert>

    <delete id="batchDelete" parameterType="Long">
        delete from proxy_group_node where group_id in
        <foreach collection="array" item="groupId" open="(" separator="," close=")">
            #{groupId}
        </foreach>
    </delete>
</mapper>